<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Creating Linkser payment method -->
    <record id="payment_method_linkser" model="payment.method">
        <field name="name">Linkser</field>
        <field name="code">linkser</field>
         <field name="image" type="base64"
               file="advanced_payment_linkser/static/src/img/logo-linkser.png"/>
        <field name="sequence">12</field>
        <field name="support_tokenization">True</field>
        <field name="support_express_checkout">False</field>
        <field name="support_refund">partial</field>
    </record>
</odoo>